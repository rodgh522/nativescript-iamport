{
	"name": "nativescript-ns-import",
	"version": "1.0.0",
	"description": "Import for nativescript",
	"main": "ns-import",
	"typings": "index.d.ts",
	"nativescript": {
		"platforms": {
			"android": "8.0.0",
			"ios": "8.0.0"
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/NativeScriptDeveloper/nativescript-ns-import.git"
	},
	"scripts": {
		"tsc": "npm i && tsc",
		"build": "npm run tsc && npm run build.native",
		"build.native": "node scripts/build-native.js",
		"tslint": "cd .. && tslint \"**/*.ts\" --config tslint.json --exclude \"**/node_modules/**\"",
		"ci.tslint": "npm i && tslint '**/*.ts' --config '../tslint.json' --exclude '**/node_modules/**' --exclude '**/platforms/**'",
		"prepack": "npm run build.native",
		"plugin.prepare": "npm run build undefined",
		"clean": " && npx rimraf -- node_modules package-lock.json && npm i"
	},
	"keywords": [
		"NativeScript",
		"JavaScript",
		"Android",
		"iOS"
	],
	"author": {
		"name": "gen",
		"email": "rodgh522@github.com"
	},
	"bugs": {
		"url": "https://github.com/NativeScriptDeveloper/nativescript-ns-import/issues"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/NativeScriptDeveloper/nativescript-ns-import",
	"devDependencies": {
		"@nativescript/core": "^8.0.0",
		"typescript": "~3.4.5",
		"prompt": "^1.0.0",
		"rimraf": "^2.6.3",
		"tslint": "^5.12.1",
		"semver": "^5.6.0"
	},
	"dependencies": {},
	"bootstrapper": "nativescript-plugin-seed"
}